@model IEnumerable<LoginScreen.Models.Usuario>
@{
    ViewData["Title"] = "Logado";
}
<div class="container" id="logado">
    <div class="header">
        <h1 id="tituloLogado">LoginScreen</h1>

        <h3 class="texto-padrao" style="display:inline; margin-left: 50px;">Bem vindo @TempData["Logado"]</h3>
        <div class="nav" style="float:right;">
            <a class="texto-padrao" asp-action="gerenciarConta">Gerenciar Conta</a>&nbsp;|&nbsp;
            <a class="texto-padrao" asp-action="Logar">Deslogar</a>
        </div>
    </div>
    <div>
        <div>
            <h1 class="tituloPagina">Cadastrados</h1>

        </div>
        <div>
            <div class="text-success" style="margin-right:100px;">@TempData["sucess"]</div>
            <table class="table table-striped table-dark table-bordered">
                <thead>
                    <tr>
                        <th>@Html.DisplayNameFor(model => model.id)</th>
                        <th>@Html.DisplayNameFor(model => model.nomeDeUsuario)</th>
                        <th>@Html.DisplayNameFor(model => model.nome)</th>
                        <th>@Html.DisplayNameFor(model => model.email)</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelitem => item.id)
                            </td>
                            <td>
                                @Html.DisplayFor(modelitem => item.nomeDeUsuario)
                            </td>
                            <td>
                                @Html.DisplayFor(modelitem => item.nome)
                            </td>
                            <td>
                                @Html.DisplayFor(modelitem => item.email)
                            </td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>